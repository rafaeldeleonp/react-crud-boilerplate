{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "globals": {
    "__DEV__": true,
    "__API__": true
  },
  "rules": {
    "react/no-multi-comp": 0,
    "react/jsx-filename-extension": 0,
    "no-debugger": 0,
    "no-plusplus": 0,
    "react/no-array-index-key": 0,
    "react/forbid-prop-types": 0,
    "react/no-danger": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "max-len": [
      "error", {
        "code": 120,
        "tabWidth": 2,
        "ignoreComments": true,
        "ignoreTrailingComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ]
  }
}
